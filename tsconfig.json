{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/constants/*": [
        "constants/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/layouts/*": [
        "layouts/*"
      ],
      "@/interfaces/*": [
        "interfaces/*"
      ],
      "@/utils/*": [
        "utils/*"
      ]
    }
  }
}
